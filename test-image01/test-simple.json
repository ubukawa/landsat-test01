{
  "version": 8,
  "name": "test-simple",
  "sources": {
    "ne-test": {
      "type": "vector",
      "tiles": ["https://ubukawa.github.io/VT-test-0614/ne-test/{z}/{x}/{y}.pbf"],
      "minzoom": 0,
      "maxzoom": 5
    },
    "landsat": {
      "type": "raster",
      "tiles": ["https://ubukawa.github.io/landsat-test01/test-image01/TMS/{z}/{x}/{y}.png"],
      "tileSize": 256,
      "attribution": "Landsat-8 image courtesy of the U.S. Geological Survey" 
      "minzoom": 6,
      "maxzoom": 12
    }
  },
  "sprite": "https://ubukawa.github.io/VT-test-0614/sprite/sprite",
  "glyphs": "https://ubukawa.github.io/VT-test-0614/font/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "maxzoom": 5,
      "paint": {"background-color": "rgba(0, 0, 0, 1)"}
    },
    {
      "id": "landmass",
      "type": "fill",
      "source": "ne-test",
      "source-layer": "landmass",
      "maxzoom": 5,
      "paint": {"fill-color": "rgba(174, 255, 147, 1)"}
    },
    {
      "id": "landsat",
      "type": "raster",
      "source": "landsat",
      "minzoom": 6,
      "maxzoom": 12
    },
    {
      "id": "coastline",
      "type": "line",
      "source": "ne-test",
      "source-layer": "coastl",
      "maxzoom": 5,
      "paint": {"line-color": "rgba(58, 0, 160, 1)"}
    },
    {
      "id": "bndl",
      "type": "line",
      "source": "ne-test",
      "source-layer": "bndl",
      "maxzoom": 5,
      "paint": {"line-color": "rgba(138, 138, 138, 1)"}
    },
    {
      "id": "popp",
      "type": "symbol",
      "source": "ne-test",
      "source-layer": "popp",
      "minzoom": 2,
      "maxzoom": 5,
      "layout": {
        "text-font": ["sans"],
        "text-field": "{NAME}",
        "icon-image": "national_capital",
        "text-offset": [1, 1]
      }
    }
  ]
}